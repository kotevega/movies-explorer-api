{
  "extends": [
    // "plugin:import/errors",
    // "plugin:import/warnings",
    "airbnb-base"
  ],
  "rules": {
    "no-console": "off",
    "no-useless-escape": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ],
    "consistent-return": "off",
    "no-control-regex": 0
  }
}
